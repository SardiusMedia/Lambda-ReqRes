{
  "name": "serverless-req-res",
  "description": "ReqRes is a light weight \"Express.js like\" Request and Response handler (with plug-in support) for Serverless Lambda functions",
  "version": "0.3.3",
  "main": "lib/index.js",
  "author": "Sardius Media",
  "repository": "sardiusmedia/serverless-ReqRes",
  "license": "MIT",
  "scripts": {
    "compile": "babel src --out-dir lib",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
